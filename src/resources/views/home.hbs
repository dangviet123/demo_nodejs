<div class="row">
    {{#each course}}
    <div class="col-md-4 card_col">
        <div class="card">
            <img class="card-img-top" src="{{this.image}}" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.description}}</p>
                <a href="/courses/{{this.slug}}" class="btn btn-primary">XÊM THÊM</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>